{
  "expo": {
    "name": "GoJuonApp",
    "slug": "GoJuonApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "assets/animations/*.gif",
      "assets/sounds/*.mp3",
      "assets/images/*",
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.gojuonapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": []
      },
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.gojuonapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "32e09690-1394-4e60-9b61-33ff6d638e4c"
      }
    },
    "owner": "yurisachan",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/32e09690-1394-4e60-9b61-33ff6d638e4c",
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
